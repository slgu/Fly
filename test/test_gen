func coolfunc(b, v) {
    v = 3;
    ret = (2 + 3) * v;
    if (ret > 10) {
        print("greater than 10");
    };

    if (ret > 20) {
        print("geater than 20");
    } else {
        print("no greater than 20");
    };

    for (i = 1; i < 10; i = i+1) {
        print(i);
    };
    j = 2;
    j = 3;

/*
    while (j < 5) {
        print(j);
        j = j+1;
    };
*/

    print(ret);
    print(v);
    print(b);

    return ret;
}

func main() {
    coolfunc("Hello world", 4);
    return 0;
}
