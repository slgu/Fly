

func main() {

    for( i = 1; i < 11; i = i+1) {
        s = fly say_hello(", World", i, 2, 3);
        register s recv_hello(i);
    }

    while(true) {

    }

    return 0;
}

func say_hello(str, idx, b, c) {
    msg = "Hello" + str;
    print(msg);

    num = idx+b*c;
    print(idx);
    print(num);
    return num;
}

func recv_hello(idx, num) {
    msg = "Got it";
    print(msg);
    print(idx);
    print(num);
}
