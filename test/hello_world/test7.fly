        print(msg);
    }

    func play() {
        age = 10;
        ans = age + 100;
        print(ans);
    }
}

class cat {
    name:String;
    age:Int;
    owner:String;
    another_dog:dog;
    func meow(to) {
        msg = "Hi " + to + " my name is " + name;
        print(msg);
    }

