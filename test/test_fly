func gen(a) {
    print(a);
    return a;
}

func recv(x) {
    print("Got it 1");
    print(x);
}

func add(a, b, c) {
    sum = a + b + c;
    return sum;
}

func get_ans(ans) {
    print("Got it 2");
    print(ans);
}

func main() {
    gen("seq 1: Hello World");
    gen("seq 2: Hello World");

    fly gen("Hello World 1");

    fly gen("Hello World 2");

    s = fly gen("Hello World 3");
    register s recv();

    s = fly gen("Hello World 4");
    register s recv();

/*    register s get_ans();*/

    s1 = fly add(1, 2, 3);
    register s1 get_ans();

    while(true) {
    };

    return 0;
}
