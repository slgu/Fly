func process(msg) {
    filename = "p1";
    res = exec(msg, filename);
    print(res);
    print(res);
    print(res);
    print(res);
    print(res);
    print(res);
    return res;
}

func send_back(con, msg) {
    con.send(msg);
}

func handle_req(con) {
    while(con.is_alive()) {
        msg = con.recv();
        s = fly process(msg);
        register s send_back(con);
    }
}

func main() {
    a = @server;
    a.listen(5567);
    while(true) {
        con = a.accept();
        fly handle_req(con);
    }
    return 0;
}
