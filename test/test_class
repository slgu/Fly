class dog {
    name:String;
    age:Int;
    owner:String;

    func bark(to) {
        msg = "Hi " + to + " my name is";
        print(msg);
        print(name);
    }

    func play() {
        ans = age + 100;
        print(ans);
    }
}

class cat {
    name:String;
    age:Int;
    owner:String;

    func meow(to) {
        msg = "Hi " + to + " my name is";
        print(msg);
        print(name);
    }

    func play() {
        ans = age + 100;
        print(ans);
    }
}

func main() {
    a = @dog;
    a.age = 12;
    a.bark("slgu");
    a.play();
    return 0;
}
