class dog {
    name:String;
    age:Int;
    owner:String;

    func bark(to) {
        msg = "Hi " + to + " my name is";
        print(msg);
        print(name);
    }

    func play() {
        ans = age + 100;
        print(ans);
    }
}

class cat {
    name:String;
    age:Int;
    owner:String;
    another_dog:dog;
    func meow(to) {
        msg = "Hi " + to + " my name is";
        print(msg);
        print(name);
        print(another_dog.age);
    }

    func play() {
        ans = age + 100;
        print(ans);
    }
}

func main() {
    d = 2;
    d = 3;
    a = @dog;
    a.age = 12;
    a.bark("slgu");
    a.play();
    c = @cat;
    c.meow("sad");
    return 0;
}
